<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Community Health Academy</h:title>
    <model>
      <instance>
        <cha_module_two delimiter="#" id="cha_module_two" prefix="J1!cha_module_two!" version="2020-09-16 01:35:54">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source/>
            <source_id/>
            <user/>
            <contact>
              <_id/>
              <phone/>
            </contact>
          </inputs>
          <patient_id/>
          <patient_phone/>
          <training_intro>
            <step_one/>
            <introduction/>
            <step_two/>
          </training_intro>
          <learning_quiz>
            <assessment_note/>
            <next_steps_q/>
            <next_steps_score/>
            <parts_to_cover_q/>
            <parts_to_cover_score/>
            <same_room_q/>
            <same_room_score/>
          </learning_quiz>
          <assessment_score/>
          <assessment_passed/>
          <summary>
            <submit/>
            <result/>
            <thank_you_note/>
            <generated_note_name_35/>
            <success_note/>
            <confidence_level/>
            <gauge_mastery/>
            <transmission_confidence/>
            <preventive_measures_confidence/>
            <feedback/>
            <comments/>
          </summary>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </cha_module_two>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/cha_module_two/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/cha_module_two/inputs/source" type="string"/>
      <bind nodeset="/cha_module_two/inputs/source_id" type="string"/>
      <bind nodeset="/cha_module_two/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/cha_module_two/inputs/contact/phone" type="string"/>
      <bind calculate="../inputs/contact/_id" nodeset="/cha_module_two/patient_id" type="string"/>
      <bind calculate="../inputs/contact/phone" nodeset="/cha_module_two/patient_phone" type="string"/>
      <bind nodeset="/cha_module_two/training_intro/step_one" readonly="true()" type="string"/>
      <bind nodeset="/cha_module_two/training_intro/introduction" readonly="true()" type="string"/>
      <bind nodeset="/cha_module_two/training_intro/step_two" readonly="true()" type="string"/>
      <bind nodeset="/cha_module_two/learning_quiz/assessment_note" readonly="true()" type="string"/>
      <bind nodeset="/cha_module_two/learning_quiz/next_steps_q" required="true()" type="select1"/>
      <bind calculate="if(selected( /cha_module_two/learning_quiz/next_steps_q ,'refer'), 1, 0)" nodeset="/cha_module_two/learning_quiz/next_steps_score" type="string"/>
      <bind nodeset="/cha_module_two/learning_quiz/parts_to_cover_q" required="true()" type="select1"/>
      <bind calculate="if(selected( /cha_module_two/learning_quiz/parts_to_cover_q ,'nose_mouth'), 1, 0)" nodeset="/cha_module_two/learning_quiz/parts_to_cover_score" type="string"/>
      <bind nodeset="/cha_module_two/learning_quiz/same_room_q" required="true()" type="select1"/>
      <bind calculate="if(selected( /cha_module_two/learning_quiz/same_room_q ,'wash_hands'), 1, 0)" nodeset="/cha_module_two/learning_quiz/same_room_score" type="string"/>
      <bind calculate="int( /cha_module_two/learning_quiz/next_steps_score ) + int( /cha_module_two/learning_quiz/parts_to_cover_score ) + int( /cha_module_two/learning_quiz/same_room_score )" nodeset="/cha_module_two/assessment_score" type="string"/>
      <bind calculate="if( /cha_module_two/assessment_score  = 3, 'yes', 'no')" nodeset="/cha_module_two/assessment_passed" type="string"/>
      <bind nodeset="/cha_module_two/summary/submit" readonly="true()" type="string"/>
      <bind nodeset="/cha_module_two/summary/result" readonly="true()" type="string"/>
      <bind nodeset="/cha_module_two/summary/thank_you_note" readonly="true()" relevant=" /cha_module_two/assessment_passed  = 'no'" required="true()" type="string"/>
      <bind nodeset="/cha_module_two/summary/generated_note_name_35" readonly="true()" relevant=" /cha_module_two/assessment_passed  = 'no'" type="string"/>
      <bind nodeset="/cha_module_two/summary/success_note" readonly="true()" relevant=" /cha_module_two/assessment_passed  = 'yes'" required="true()" type="string"/>
      <bind nodeset="/cha_module_two/summary/confidence_level" readonly="true()" relevant=" /cha_module_two/assessment_passed  = 'yes'" required="true()" type="string"/>
      <bind nodeset="/cha_module_two/summary/gauge_mastery" readonly="true()" relevant=" /cha_module_two/assessment_passed  = 'yes'" required="true()" type="string"/>
      <bind nodeset="/cha_module_two/summary/transmission_confidence" relevant=" /cha_module_two/assessment_passed  = 'yes'" required="true()" type="select1"/>
      <bind nodeset="/cha_module_two/summary/preventive_measures_confidence" relevant=" /cha_module_two/assessment_passed  = 'yes'" required="true()" type="select1"/>
      <bind nodeset="/cha_module_two/summary/feedback" relevant=" /cha_module_two/assessment_passed  = 'yes'" type="string"/>
      <bind nodeset="/cha_module_two/summary/comments" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/cha_module_two/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/cha_module_two/inputs">
      <label>Patient</label>
      <input appearance="hidden" ref="/cha_module_two/inputs/source">
        <label>Source</label>
      </input>
      <input appearance="hidden" ref="/cha_module_two/inputs/source_id">
        <label>Source ID</label>
      </input>
      <group ref="/cha_module_two/inputs/user">
        <label>User</label>
      </group>
      <group ref="/cha_module_two/inputs/contact">
        <label>Contact</label>
        <input appearance="hidden" ref="/cha_module_two/inputs/contact/_id">
          <label>_id</label>
        </input>
        <input appearance="hidden" ref="/cha_module_two/inputs/contact/phone">
          <label>Phone</label>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/cha_module_two/training_intro">
      <label>COVID-19 Care</label>
      <input ref="/cha_module_two/training_intro/step_one">
        <label>Step 1:
Open Oppia Mobile to read the course content. Once complete, please return to this task for Step 2.</label>
      </input>
      <input ref="/cha_module_two/training_intro/introduction">
        <label>This second module, **COVID Care**, explains the role of a Community Health Worker in recognizing and managing COVID-19: and educating others on how to care for a patient with COVID-19.</label>
      </input>
      <input ref="/cha_module_two/training_intro/step_two">
        <label>Step 2:
Complete a short assessment on the course material.</label>
      </input>
    </group>
    <group appearance="field-list" ref="/cha_module_two/learning_quiz">
      <label>Assessment</label>
      <input ref="/cha_module_two/learning_quiz/assessment_note">
        <label>We would like to ask you a few questions to understand your knowledge on COVID-19 with regards to this module:</label>
      </input>
      <select1 ref="/cha_module_two/learning_quiz/next_steps_q">
        <label>1. A patient has fast breathing, difficulty speaking and bluish lips, or has a fever, is confused and cannot stay awake. What should you do next?</label>
        <item>
          <label>Tell patient to stay at home and lie down</label>
          <value>stay_home</value>
        </item>
        <item>
          <label>Provide oral rehydration therapy</label>
          <value>oral_rehydration</value>
        </item>
        <item>
          <label>Provide cough medication</label>
          <value>cough_medication</value>
        </item>
        <item>
          <label>Advise patient to take acetaminophen</label>
          <value>acetaminophen</value>
        </item>
        <item>
          <label>Refer to local health center for emergency help</label>
          <value>refer</value>
        </item>
      </select1>
      <select1 ref="/cha_module_two/learning_quiz/parts_to_cover_q">
        <label>2. When caring for a suspected COVID-19 patient, what are the two most important parts of your body to cover?</label>
        <item>
          <label>Nose and mouth</label>
          <value>nose_mouth</value>
        </item>
        <item>
          <label>Nose and ears</label>
          <value>nose_ears</value>
        </item>
        <item>
          <label>Hands and feet</label>
          <value>hands_feet</value>
        </item>
        <item>
          <label>Hands and hair</label>
          <value>hands_hair</value>
        </item>
      </select1>
      <select1 ref="/cha_module_two/learning_quiz/same_room_q">
        <label>3. What should you always do immediately after being in the same room as a COVID-19 patient?</label>
        <item>
          <label>Take your temperature</label>
          <value>temperature</value>
        </item>
        <item>
          <label>Wash your hands with soap and running water</label>
          <value>wash_hands</value>
        </item>
        <item>
          <label>Wipe your mouth and nose</label>
          <value>wipe_nose_mouth</value>
        </item>
        <item>
          <label>Drink water</label>
          <value>drink_water</value>
        </item>
      </select1>
    </group>
    <group appearance="field-list summary" ref="/cha_module_two/summary">
      <label></label>
      <input ref="/cha_module_two/summary/submit">
        <label>&lt;h4 style="text-align:center;"&gt;Be sure you Submit to complete this action.&lt;/h4&gt;</label>
      </input>
      <input appearance="h1 green" ref="/cha_module_two/summary/result">
        <label>Thank you</label>
      </input>
      <input ref="/cha_module_two/summary/thank_you_note">
        <label>Good start, but you only answered <output value=" /cha_module_two/assessment_score "/>** out of the 3 questions correctly.
This task will not be completed until all the questions are answered correctly.</label>
      </input>
      <input ref="/cha_module_two/summary/generated_note_name_35">
        <label>**Do you need additional support on this topic?**</label>
        <hint>Optional</hint>
      </input>
      <input ref="/cha_module_two/summary/success_note">
        <label>Thank you for completing Module 2: COVID Care. You **scored <output value=" /cha_module_two/assessment_score "/>** out of 3. Great Job, you've completed Module 2! You can revisit this content at any time in your Oppia app.</label>
      </input>
      <input ref="/cha_module_two/summary/confidence_level">
        <label>**The following questions are designed to understand your confidence to perform the tasks taught in this lesson.**</label>
      </input>
      <input ref="/cha_module_two/summary/gauge_mastery">
        <label>**Following this training, what is your level of confidence that you can effectively perform the following tasks related to COVID-19?**</label>
      </input>
      <select1 ref="/cha_module_two/summary/transmission_confidence">
        <label>1. Recognizing and managing COVID-19 cases</label>
        <item>
          <label>Not confident at all</label>
          <value>no_confidence</value>
        </item>
        <item>
          <label>Not very confident</label>
          <value>a_little</value>
        </item>
        <item>
          <label>Somewhat confident</label>
          <value>somewhat</value>
        </item>
        <item>
          <label>Very confident</label>
          <value>very</value>
        </item>
      </select1>
      <select1 ref="/cha_module_two/summary/preventive_measures_confidence">
        <label>2. Caring for the sick</label>
        <item>
          <label>Not confident at all</label>
          <value>no_confidence</value>
        </item>
        <item>
          <label>Not very confident</label>
          <value>a_little</value>
        </item>
        <item>
          <label>Somewhat confident</label>
          <value>somewhat</value>
        </item>
        <item>
          <label>Very confident</label>
          <value>very</value>
        </item>
      </select1>
      <input ref="/cha_module_two/summary/feedback">
        <label>Do you have any other questions or feedback on this topic? (Optional)</label>
        <hint>Optional</hint>
      </input>
      <input ref="/cha_module_two/summary/comments">
        <label>Write them down here and your supervisor will follow up</label>
      </input>
    </group>
  </h:body>
</h:html>
